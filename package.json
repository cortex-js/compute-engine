{
  "version": "0.53.0",
  "scripts": {
    "doc": "bash ./scripts/doc.sh",
    "build": "bash ./scripts/build.sh",
    "clean": "bash ./scripts/clean.sh",
    "coverage": "bash ./scripts/test.sh coverage",
    "dist": "bash ./scripts/build.sh production; bash ./scripts/test.sh",
    "prepare": "bash ./scripts/build.sh production",
    "start": "bash ./scripts/start.sh",
    "snapshot": "bash ./scripts/test.sh snapshot",
    "version": "bash ./scripts/version.sh",
    "test": "bash ./scripts/test.sh",
    "check:deps": "madge --circular --extensions ts src/compute-engine && echo 'No runtime circular dependencies found (type-only cycles are expected)'",
    "lint": "eslint src --ext .ts --fix && prettier --write 'src/**/*.{ts,js,css,md}'",
    "typecheck": "bash ./scripts/typecheck.sh"
  },
  "name": "@cortex-js/compute-engine",
  "description": "Symbolic computing and numeric evaluations for JavaScript and Node.js",
  "homepage": "https://cortexjs.io/compute-engine/",
  "repository": {
    "type": "git",
    "url": "https://github.com/cortex-js/compute-engine.git"
  },
  "bugs": "https://github.com/cortex-js/compute-engine/issues/",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "math",
    "JSON",
    "mathjs",
    "mathematica",
    "maple",
    "algebra",
    "symbolic computing",
    "scientific computing",
    "sympy"
  ],
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/compute-engine.d.ts",
      "import": "./dist/compute-engine.min.esm.js",
      "require": "./dist/compute-engine.min.umd.js",
      "default": "./dist/compute-engine.min.esm.js"
    },
    "./math-json": {
      "types": "./dist/types/math-json.d.ts",
      "import": "./dist/math-json.min.esm.js",
      "require": "./dist/math-json.min.umd.js",
      "default": "./dist/math-json.min.esm.js"
    }
  },
  "main": "./dist/compute-engine.min.esm.js",
  "module": "./dist/compute-engine.min.esm.js",
  "browser": "./dist/compute-engine.min.umd.js",
  "unpkg": "./dist/compute-engine.min.umd.js",
  "types": "./dist/types/compute-engine.d.ts",
  "engines": {
    "npm": ">=10.5.0",
    "node": ">=21.7.3"
  },
  "devDependencies": {
    "@cortex-js/prettier-config": "^1.2.0",
    "@jest/console": "^29.7.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@typescript-eslint/typescript-estree": "^8.56.0",
    "chalk": "^5.6.2",
    "check-node-version": "^4.2.1",
    "concat-md": "^0.5.1",
    "esbuild": "^0.27.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-unsanitized": "^4.1.5",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^29.7.0",
    "jest-silent-reporter": "^0.6.0",
    "madge": "^8.0.0",
    "open": "^11.0.0",
    "prettier": "^3.8.1",
    "prettier-2": "npm:prettier@^2.8.8",
    "serve-http": "^1.0.7",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.17",
    "typedoc-plugin-markdown": "^4.10.0",
    "typedoc-plugin-no-inherit": "^1.6.1",
    "typescript": "^5.9.3",
    "update-notifier": "^7.3.1"
  },
  "prettier": "@cortex-js/prettier-config",
  "dependencies": {
    "@arnog/colors": "^0.2.1",
    "complex-esm": "^2.1.1-esm1",
    "decimal.js": "^10.6.0"
  }
}
